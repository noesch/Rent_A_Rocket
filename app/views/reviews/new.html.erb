<div class="container">
  <h1>Leave a review</h1>
  <%= link_to "Back to all rockets", rockets_path, class: "btn btn-primary my-2" %>
  <%= link_to "Show my bookings", bookings_path, class: "btn btn-primary my-2" %>

  <%= simple_form_for [@booking, @review] do |f|%>
  <%= f.input :comment %>
  <%= f.input :rating %>
  <%= f.submit "create", class: "btn btn-primary my-2"%>
  <% end %>
</div>
